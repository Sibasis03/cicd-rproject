pipeline {
  agent {
        docker { 
          image 'sibasis03/dock-agent:v1' 
          args '--user root -v /var/run/docker.sock:/var/run/docker.sock'
        }
    } 

  stages {
    stage ('checkout') {
      steps {
        sh 'echo passed'
      }
    }

    stage ('Build & Test') {
      steps {
        sh 'mvn clean package'
      }
    }

    
  }
}